    "@vue/cli-plugin-eslint": "~4.5.3",
    "@vue/cli-plugin-router": "^4.5.19",
    "@vue/cli-service": "~4.5.3",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
